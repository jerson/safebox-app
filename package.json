{
  "name": "SafeBox",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "tsc && tslint -p .",
    "prettier": "prettier --write --single-quote 'src/**/*.tsx'",
    "docs": "typedoc --excludeExternals --out docs --exclude '**/*/*.test.tsx' --mode file ./src "
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "0.9.6",
    "@react-native-community/async-storage": "1.5.1",
    "@react-native-community/netinfo": "4.1.3",
    "@react-native-community/slider": "2.0.0",
    "@react-native-community/viewpager": "1.1.7",
    "crypto-js": "3.1.9-1",
    "google-protobuf": "3.9.0",
    "lottie-ios": "3.0.3",
    "lottie-react-native": "3.0.3",
    "moment": "2.24.0",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-vector-icons": "6.6.0",
    "react-native-webview": "5.12.1",
    "react-navigation": "3.11.1",
    "realm": "2.29.1",
    "tinycolor2": "1.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/runtime": "7.5.5",
    "@types/google-protobuf": "3.7.1",
    "@types/jest": "24.0.15",
    "@types/react": "16.8.23",
    "@types/react-native": "0.60.2",
    "@types/react-test-renderer": "16.8.2",
    "babel-jest": "24.8.0",
    "husky": "3.0.1",
    "jest": "24.8.0",
    "lint-staged": "9.2.0",
    "metro-react-native-babel-preset": "0.55.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6",
    "ts-protoc-gen": "0.10.0",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-react": "4.0.0",
    "tslint-react-native": "0.0.7",
    "typedoc": "0.14.2",
    "typescript": "3.5.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "git add"
    ],
    "*.tsx": [
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test --bail && lint-staged"
    }
  }
}
